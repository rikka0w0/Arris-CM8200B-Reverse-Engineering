msgstr "   aconnect -i|-o [-オプション]\n"

#: seq/aconnect/aconnect.c:61
#, c-format
msgid "     -i,--input          list input (readable) ports\n"
msgstr "     -i,--input          入力 (読込) ポートの一覧\n"

#: seq/aconnect/aconnect.c:62
#, c-format
msgid "     -o,--output         list output (writable) ports\n"
